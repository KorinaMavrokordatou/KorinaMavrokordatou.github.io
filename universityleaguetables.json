{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
 "title": {
    "text": "University League Tables 2022",
    "subtitle": [
      "Overall University Score based on Entry Standards, Student Satisfaction, Reasearch Quality",
      "and Graduate Prospects. Data Source: https://www.thecompleteuniversityguide.co.uk"
    ],
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "middle"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/KorinaMavrokordatou/KorinaMavrokordatou.github.io/main/overall_rankings_uk(3).csv",
    "format": {"type": "csv"}
  },
  "height": 300,
  "width": 300,
  "background": "ghostwhite",
 "mark": {"type": "line", "point": {"fill":"ghostwhite"}},
  "encoding": {
    "x": {
      "field": "university",
      "type": "nominal",
      "title": null,
      "axis": {"labelAngle": -75}
          },
    "y": {
      "field": "score",
      "type": "quantitative",
      "title": "Score, %",
      "scale": {"domain": [60, 100]},
      "axis": {"grid": false},
      "sort": "-y"
    },
    "color": {
      "field": "type",
      "type": "nominal",
      "scale": {"scheme": "tableau10"},
      "title": null,
      "legend": {"orient": "right"}
    }
  },
  "config": {}
} 
